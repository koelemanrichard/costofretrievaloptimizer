# Supabase Migration Environment Variables
# Copy this file to .env.migration and fill in your values

# ============================================
# SOURCE PROJECT (Current/Old Supabase Project)
# ============================================
# Find these in Supabase Dashboard → Settings → API
SOURCE_SUPABASE_URL=https://YOUR_OLD_PROJECT_REF.supabase.co
SOURCE_SUPABASE_ANON_KEY=your_old_anon_key
SOURCE_SUPABASE_SERVICE_ROLE_KEY=your_old_service_role_key

# ============================================
# TARGET PROJECT (New Supabase Pro Project)
# ============================================
# Find these in Supabase Dashboard → Settings → API
TARGET_SUPABASE_URL=https://YOUR_NEW_PROJECT_REF.supabase.co
TARGET_SUPABASE_ANON_KEY=your_new_anon_key
TARGET_SUPABASE_SERVICE_ROLE_KEY=your_new_service_role_key

# ============================================
# BACKUP SETTINGS
# ============================================
# Directory for backups (relative to project root)
BACKUP_DIR=./backup
